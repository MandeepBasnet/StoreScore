VITE_APPWRITE_DATABASE_ID=[YOUR_DATABASE_ID]
VITE_APPWRITE_COLLECTION_STORES=[YOUR_STORES_COLLECTION_ID]
VITE_APPWRITE_COLLECTION_MANAGERS=[YOUR_MANAGERS_COLLECTION_ID]
VITE_APPWRITE_COLLECTION_KPI=[YOUR_KPI_COLLECTION_ID]